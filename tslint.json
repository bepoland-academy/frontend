{
  "extends": ["tslint-eslint-rules", "tslint:latest"],
  "rules": {
    "no-submodule-imports": [
      true,
      "rxjs",
      "@angular/platform-browser",
      "@angular/core/testing",
      "@angular/common/http"
    ],
    "typedef": [
      true,
      "call-signature"
    ],
    "noImplicitAny": false,
    "max-line-length": [
      true,
      140
    ],
  }
    
}
